{
  "version": 2,
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "/backend/server.py"
    },
    {
      "src": "/(.*)",
      "dest": "/frontend/build/$1"
    }
  ],
  "functions": {
    "backend/server.py": {
      "runtime": "python3.9"
    }
  },
  "env": {
    "MONGO_URL": "@mongo-url",
    "DB_NAME": "@db-name"
  }
}